# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]       [fsoptions2]

/boot       bml     boot
/recovery   bml     recovery
/cache      ext4   /dev/block/stl11        NULL        ext4         NULL              rw,nosuid,nodev,check=no
/data       ext4    /dev/block/stl10        NULL        ext4         NULL              rw,nosuid,nodev,check=no
/sdcard     vfat    /dev/block/mmcblk0p1
/sd-ext     ext3    /dev/block/mmcblk0p2
/system     ext4    /dev/block/stl9         NULL        ext4         NULL              check=no
